@{
    ViewData["Title"] = "Details";
    ViewData["SubTitle"] = Model?.GetType()?.Name?.Replace("Page", string.Empty) ?? "Unspecified";  
    Layout = "_Layout";
    var itemId = "";
    var str = Model?.GetType()?.Name?.Replace("Page", string.Empty) ?? "Unspecified";
    if(str == "Students")
    {
        itemId = Model?.Student?.Id ?? string.Empty;
    }
    else
    {
        itemId = Model?.TeamWork?.Id ?? string.Empty;
    } 
}
 }

 <h1>@ViewData["Title"]</h1>

<div>
    <h4>@ViewData["SubTitle"]</h4>
    <hr />
    @RenderBody()
</div>
<div>
    <a asp-page="./Edit" asp-page-handler="Edit" asp-route-id="@itemId">Edit</a> |
    <a asp-page="./Index" asp-page-handler="Index">Back to List</a>
</div>

